<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="boldTitleText">
        <item name="android:fontFamily">nunito-bold</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/title_text_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="boldRegularText">
        <item name="android:fontFamily">nunito-bold</item>
        <item name="android:textSize">@dimen/regular_text_size</item>
        <item name="android:textColor">@color/deep_purple_200_dark</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="boldLargeText">
        <item name="android:fontFamily">nunito-bold</item>
        <item name="android:textSize">@dimen/large_text_size</item>
        <item name="android:textColor">@color/deep_purple_200_dark</item>
        <item name="android:textAlignment">center</item>
    </style>
</resources>